TARGETNAME=MinFan
TARGETPATH=..\..\$(_OBJ_DIR)
TARGETTYPE=PROGRAM

C_DEFINES=-DUSE_DEBUG_CRTS

UMENTRY=main
UMAPPL=MinFan
UMTYPE=console
UMLIBS=\
             $(SDK_LIB_PATH)\kernel32.lib	\
             $(TARGETPATH)\*\LKRhash.lib	\

USE_MSVCRT=1
USE_STL=1

INCLUDES=.;..\..\inc;	    \

SOURCES= \
        minfan.cpp \
