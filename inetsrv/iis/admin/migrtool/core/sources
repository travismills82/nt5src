			$(SDK_LIB_PATH)\uuid.lib \
			$(SDK_LIB_PATH)\fci.lib \
			$(SDK_LIB_PATH)\msxml2.lib \
			$(SDK_LIB_PATH)\setupapi.lib \
			$(SDK_LIB_PATH)\shlwapi.lib \
			$(SDK_LIB_PATH)\crypt32.lib
