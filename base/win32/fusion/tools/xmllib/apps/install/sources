	$(SDK_LIB_PATH)\ntdll.lib \
	$(SDK_LIB_PATH)\user32.lib \
	$(SDK_LIB_PATH)\kernel32.lib \
	$(RTL_XML_LIB_PATH)\rtlsxsutils.lib \
