MAJORCOMP=windows
MINORCOMP=dm

TARGETNAME=dm
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM
C_DEFINES = $(C_DEFINES) -DUNICODE -D_UNICODE
SOURCES=dm.rc   \
        dm.c

UMENTRY=main
UMTYPE=console
UMAPPL=dm


TARGETLIBS= $(SDK_LIB_PATH)\cabinet.lib \
            $(SDK_LIB_PATH)\ntdll.lib \
