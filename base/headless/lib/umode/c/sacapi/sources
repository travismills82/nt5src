MAJORCOMP=
MINORCOMP=

TARGETNAME=sacapi
TARGETPATH=obj
TARGETTYPE=LIBRARY

UMTYPE=windows
UMENTRY=winmain

SOURCES=sacapi.c      \
        sacapi.rc     \

INCLUDES=\
         $(WINDOWS_INC_PATH);\
         $(BASE_INC_PATH);\
         $(DRIVERS_INC_PATH); \
         $(DDK_INC_PATH); \
         
TARGETLIBS =                                    \
        $(SDK_LIB_PATH)\kernel32.lib            \

C_DEFINES= $(C_DEFINES) -DUNICODE -D_UNICODE

MSC_WARNING_LEVEL=/W4 /WX


