    $(SDK_LIB_PATH)\htmlhelp.lib                \
    $(SDK_LIB_PATH)\comctl32.lib                \
    $(SDK_LIB_PATH)\gdi32.lib                   \
    $(SDK_LIB_PATH)\ntdsapi.lib                 \
    $(SDK_LIB_PATH)\crypt32.lib                 \