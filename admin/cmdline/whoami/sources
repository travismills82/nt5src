UMAPPL=whoami
UMLIBS= \
       $(ADMIN_LIB_PATH)\cmdline.lib \
       $(UMLIBS)                     \
       $(SDK_LIB_PATH)\user32.lib    \
       $(SDK_LIB_PATH)\mpr.lib       \
       $(SDK_LIB_PATH)\Secur32.lib   \
       $(SDK_LIB_PATH)\ws2_32.lib    \
