TARGETNAME=sspi
TARGETPATH=obj
TARGETTYPE=PROGRAM


PRECOMPILED_CXX=1
PRECOMPILED_INCLUDE=precomp.hxx
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj

TARGETLIBS = ..\lib\$(O)\sspilib.lib      \
            $(SDK_LIB_PATH)\kernel32.lib  \
            $(SDK_LIB_PATH)\ntdll.lib     \
            $(SDK_LIB_PATH)\Ws2_32.lib    \
            $(SDK_LIB_PATH)\security.lib  \
            $(DS_LIB_PATH)\rsa32.lib      \

USE_MSVCRT=1

UMTYPE=windows

USE_NATIVE_EH=1
USE_RTTI=1
NO_NTDLL=1
USE_NTDLL=0

C_DEFINES=$(C_DEFINES) -DRPC_NO_WINDOWS_H -DUNICODE -D_UNICODE
DELAYLOAD=user32.dll;ADVAPI32.dll;Security.dll;WS2_32.dll
DLOAD_ERROR_HANDLER=sspi.exe

# ICF doesn't do any good for this image.  Turn it off.
LINKER_NOICF=1

INCLUDES=..\inc;..\lib;              \
         $(DS_GLOBAL_INC);           \
         $(NTDS_INC);                \
         $(SECURITY_INC);            \
         $(BASE_INC_PATH);           \
         $(COM_INC_PATH);            \
         $(DS_INC_PATH);             \
         $(DS_INC_PATH)\crypto;      \
         $(DRIVERS_INC_PATH);        \
         $(ENDUSER_INC_PATH);        \
         $(WINDOWS_INC_PATH);        \
         $(NET_INC_PATH);

SOURCES=sspi.cxx                     \
        sspicli.cxx                  \
        sspisrv.cxx                  \
        msvsharelevelcli.cxx         \
        msvsharelevelsrv.cxx         \
        main.cxx                     \
