            $(SDK_LIB_PATH)\kernel32.lib  \
            $(SDK_LIB_PATH)\ntdll.lib     \
            $(DS_LIB_PATH)\secur32p.lib   \
            $(DS_LIB_PATH)\rsa32.lib      \
            $(SDK_LIB_PATH)\msasn1.lib    \
            $(SDK_LIB_PATH)\cryptdll.lib  \
            $(SDK_LIB_PATH)\crypt32.lib   \
            $(SDK_LIB_PATH)\user32.lib    \
