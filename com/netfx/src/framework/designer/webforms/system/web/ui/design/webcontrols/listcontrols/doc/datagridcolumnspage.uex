<docs>
   <doc for="DataGridColumnsPage">
 <summary>
   The Data page for the DataGrid control
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.DataGridColumnsPage">
 <summary>
   Creates a new instance of DataGridColumnsPage.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.InitForm">
 <summary>
    Creates the UI of the page.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.InitPage">
 <summary>
   Initializes the page before it can be loaded with the component.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.LoadColumnProperties">
 <summary>
   Loads the properties of a column into the ui
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.LoadColumns">
 <summary>
   Loads the columns collection
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.LoadComponent">
 <summary>
   Loads the component into the page.
 </summary>
 <seealso cref="System.Windows.Forms.Design.ComponentEditorPage"/>
   </doc>
   <doc for="DataGridColumnsPage.LoadDataSourceItem">
 <summary>
   Loads the selected datasource
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.LoadDataSourceFields">
 <summary>
   Loads the fields present in the selected datasource
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.LoadAvailableColumnsTree">
 <summary>
    Loads the fixed nodes in the available columns tree, i.e., the
    DataSource, Button and HyperLink nodes
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnChangedColumnProperties">
 <summary>
    Handles changes to the column properties made in the column node editor.
    Sets a flag to indicate there are pending changes.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnCheckChangedAutoColumn">
 <summary>
    Handles changes to the auto column generation choice.
    When this functionality is turned on, the columns collection is
    cleared, and auto generated columns are shown. When it is turned
    off, nothing is done, which effectively makes the auto generated
    columns part of the column collection.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnClickAddColumn">
 <summary>
    Adds a column to the column collection
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnClickColHeaderImagePicker">
 <summary>
   Handles clicks on the column image picker button.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnClickDeleteColumn">
 <summary>
   Deletes a column from the column collection.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnClickMoveColumnDown">
 <summary>
   Move a column down within the column collection
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnClickMoveColumnUp">
 <summary>
   Move a column up within the column collection
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnClickTemplatize">
 <summary>
   Converts a column into an equivalent template column.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnLostFocusColHeaderText">
 <summary>
    Handles focus lost on the column header text edit box, so the
    column name can be changed in the selected columns tree.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnSelChangedAvailableColumns">
 <summary>
    Handles selection change in the available columns tree.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnSelIndexChangedSelColumnsList">
 <summary>
    Handles selection change within the selected columns list.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.OnTextChangedColHeaderText">
 <summary>
    Handles changes made to the column header text.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.SaveColumnProperties">
 <summary>
   Saves the properties of a column from the ui
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.SaveComponent">
 <summary>
   Saves the component loaded into the page.
 </summary>
 <seealso cref="System.Windows.Forms.Design.ComponentEditorPage"/>
   </doc>
   <doc for="DataGridColumnsPage.SetComponent">
 <summary>
   Sets the component that is to be edited in the page.
 </summary>
 <seealso cref="System.Windows.Forms.Design.ComponentEditorPage"/>
   </doc>
   <doc for="DataGridColumnsPage.ShowHelp">
 <summary>
 </summary>
 <seealso cref="System.Windows.Forms.Design.ComponentEditorPage"/>
   </doc>
   <doc for="DataGridColumnsPage.UpdateEnabledVisibleState">
 <summary>
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.AvailableColumnNode">
 <summary>
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.DataSourceNode">
 <summary>
   This represents the datasource in the available columns tree.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.DataFieldNode">
 <summary>
   This represents a datafield available in the selected datasource within
   in the available columns tree.
   It could also represent the pseudo column implying all datafields.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.ButtonNode">
 <summary>
   This represents a button column in the available columns tree.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.EditCommandNode">
 <summary>
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.HyperLinkNode">
 <summary>
   This represents a HyperLink column in the available columns tree.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.TemplateNode">
 <summary>
   This represents a template column in the available columns tree.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.ColumnItem">
 <summary>
   Represents a column in the columns collection of the DataGrid.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.BoundColumnItem">
 <summary>
    Represents a column bound to a datafield.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.ButtonColumnItem">
 <summary>
   Represents a column containing a button.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.HyperLinkColumnItem">
 <summary>
   Represents a column containing a hyperlink.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.TemplateColumnItem">
 <summary>
   Represents a column containing a template.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.EditCommandColumnItem">
 <summary>
   Represents a EditCommandColumn
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.CustomColumnItem">
 <summary>
   Represents a column of an unknown/custom type.
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.ColumnItemEditor">
 <summary>
   Panel that provides UI to edit a column's properties
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.BoundColumnEditor">
 <summary>
   Panel that provides UI to edit a bound column's properties
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.ButtonColumnEditor">
 <summary>
   Panel that provides UI to edit a button column's properties
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.HyperLinkColumnEditor">
 <summary>
   Panel that provides UI to edit a hyperlink column's properties
 </summary>
   </doc>
   <doc for="DataGridColumnsPage.EditCommandColumnEditor">
 <summary>
   Panel that provides UI to edit a EditCommandColumn column's properties
 </summary>
   </doc>
