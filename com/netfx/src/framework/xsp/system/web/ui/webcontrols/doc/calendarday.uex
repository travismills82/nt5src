		<seealso cref="System.Web.UI.WebControls.DayRenderEventArgs"/>
		<seealso cref="System.Web.UI.WebControls.DayRenderEventArgs.Day"/>
		<seealso cref="System.Web.UI.WebControls.CalendarDay.IsSelectable"/>
