		<seealso cref="System.Data.SqlClient.SqlError.Server"/>
		<seealso cref="System.Data.SqlClient.SqlError.Source"/>
		<seealso cref="System.Data.SqlClient.SqlError.Message"/>
		<seealso cref="System.Data.SqlClient.SqlError.Procedure"/>
		<seealso cref="System.Data.SqlClient.SqlError.LineNumber"/>
	</doc><doc for="SqlError.State">
