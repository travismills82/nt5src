	</doc>
	<doc for="AssemblyProductAttribute">
		<summary>
			<para>Defines a product name custom attribute for an assembly manifest.</para>
		</summary>
		<seealso topic="cpconassemblies"/>
		<seealso topic="cpconattributesoverview"/>
	</doc>
	<doc for="AssemblyConfigurationAttribute">
		<summary>
			<para>Defines an assembly configuration custom attribute (such as retail or debug) for an assembly manifest.</para>
		</summary>
		<seealso topic="cpconassemblies"/>
		<seealso topic="cpconattributesoverview"/>
	</doc>
	<doc for="AssemblyDefaultAliasAttribute">
		<summary>
			<para>Defines a friendly default alias for an assembly manifest.</para>
		</summary>
		<remarks>
			<para> This is a friendly default alias in cases where the assembly name is not friendly
      or is a GUID. </para>
		</remarks>
		<seealso topic="cpconassemblies"/>
		<seealso topic="cpconattributesoverview"/>
	</doc>
	<doc for="AssemblyInformationalVersionAttribute">
		<summary>
			<para> Defines additional version information for an assembly manifest.</para>
		</summary>
		<remarks>
			<para>The attribute defined by this class attaches additional version information
      to an assembly for documentation purposes only. This data is never used at
      runtime. </para>
		</remarks>
		<seealso topic="cpconassemblies"/>
		<seealso topic="cpconattributesoverview"/>
	</doc>
	<doc for="AssemblyCompanyAttribute">
		<summary>
			<para>Defines a company name custom attribute for an assembly manifest.</para>
		</summary>
		<example>
			<codelink SampleID="AssemblyBuilder_DefineVersionInfoResource" SnippetID="1"/>
		</example>
		<seealso topic="cpconassemblies"/>
		<seealso topic="cpconattributesoverview"/>
	</doc>
	<doc for="AssemblyDescriptionAttribute">
		<summary>
			<para>Defines an assembly description custom attribute for an assembly manifest.</para>
		</summary>
		<example>
			<codelink SampleID="AssemblyBuilder_DefineVersionInfoResource" SnippetID="1"/>
		</example>
		<seealso topic="cpconassemblies"/>
		<seealso topic="cpconattributesoverview"/>
	</doc>
	<doc for="AssemblyTitleAttribute">
		<summary>
			<para>Defines an assembly title custom attribute for an assembly manifest.</para>
		</summary>
		<remarks>
			<para>The assembly title is a friendly name, which can include
      spaces.</para>
		</remarks>
		<example>
			<codelink SampleID="AssemblyBuilder_DefineVersionInfoResource" SnippetID="1"/>
		</example>
		<seealso topic="cpconassemblies"/>
		<seealso topic="cpconattributesoverview"/>
	</doc>
