!include $(NTMAKEENV)\sources.cool
!include $(NTMAKEENV)\sources.cor

!ifdef PASS0ONLY
NTTARGETFILE0   = 
!else
!ifdef LINKONLY
NTTARGETFILE1   = SoapSuds.Resources cool_target
!else
NTTARGETFILE0   = 
!endif
!endif

MINORCOMP       = Cool
TARGETNAME      = SoapSuds
TARGETPATH      = $(TARGETCORLIB)\$(TARGET_DIRECTORY)\$(DDKBUILDENV)
TARGETTYPE      = PROGRAM
SOURCES         =
NO_BROWSER_FILE = 1

SYNCHRONIZE_DRAIN=1

# Set version information in assembly manifest
COOL_SET_VERSION = 1 

COOL_SOURCE_DIR  =  $(CORBASE)\src\toolbox\SoapSuds
COOL_TARGET_TYPE  = PROGRAM
COOL_INCLUDE_FILE = $(CORENV)\bin\smc_debug.pp

SOAPSUDS_IMPORTS = \
  /r:SoapSudsCode.dll \
  /r:System.Runtime.Remoting.dll 

WIN32_RESOURCE_FILE = SoapSuds.rc
COOL_COMPILE_FLAGS = /res:$(O)\SoapSuds.resources $(SOAPSUDS_IMPORTS)
COOL_EXTRA_COMPILE_FLAGS = /C:R SoapSuds.res
COOL_EXTRA_PREPROCESS_FLAGS = /FI ..\..\..\inc\version\__file__.ver

# This is a nasty hack so we can preproess this file with our version stamp.
COOL_SOURCES = \
    Resource.cs \
    SoapSuds.cs \

