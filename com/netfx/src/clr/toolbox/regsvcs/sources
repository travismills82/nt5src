!include $(NTMAKEENV)\sources.cool
!include $(NTMAKEENV)\sources.cor

!ifdef PASS0ONLY
NTTARGETFILE0   = 
!else
!ifdef LINKONLY
NTTARGETFILE1   = cool_target CopyManagedPdb
!else
NTTARGETFILE0   = 
!endif
