!include $(NTMAKEENV)\sources.cor

INCLUDES        =$(INCLUDES);..\..\inc;..\..\debug\inc;
!ifdef _BUILD_WINCE
INCLUDES    =$(INCLUDES);$(CORENV)\inc
!else
INCLUDES    =..\strike\inc;$(INCLUDES)
!endif

MINORCOMP   =strikeng

COFFBASE    =$(MINORCOMP)
