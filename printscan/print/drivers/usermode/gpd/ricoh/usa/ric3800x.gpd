    *Name: "Slip Sheet"
*Else:
    *rcNameID: =RC_STR_SLIPSHEET
*Endif:
    *HelpIndex: =HLP_SLIPSHEET
    *ConflictPriority: 10
    *DefaultOption: OFF
    *Option: OFF
    {
        *rcNameID: =OFF_DISPLAY
        *Command: CmdSelect
        {
            *Order: =PJL_SETUP_90
            *Cmd: "@PJL SET SLIPSHEETPRINT=OFF<0A>"
        }
    }
    *Option: ON
    {
        *rcNameID: =ON_DISPLAY
        *switch: InputBin
        {
            *case: BypassTray
            {
                *Command: CmdSelect
                {
                    *Order: =PJL_SETUP_90
                    *Cmd: "@PJL SET AUTOTRAYCHANGE=OFF<0A>@PJL SET SLIPSHEETPRINT=BLANK<0A>"
+                         "@PJL SET DOCBODYTRAY=BYPASS<0A>"
                }
            }
            *default:
            {
                *Command: CmdSelect
                {
                    *Order: =PJL_SETUP_90
                    *Cmd: "@PJL SET SLIPSHEETPRINT=OFF<0A>"
                }
            }
        }
    }
}

