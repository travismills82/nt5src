    *Option: 32MB_NotInstalled
    {
        *rcNameID: =RC_STR_32MB_NOTINSTALLED
        *DisabledFeatures: LIST(Collate)
    }
    *Option: 32MB_Installed
    {
        *rcNameID: =RC_STR_32MB_INSTALLED
    }
