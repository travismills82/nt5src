TARGETNAME=tracectr
TARGETPATH=obj
TARGETTYPE=LIBRARY

ATL_VER=30
USE_ATL=1

PASS0_PUBLISH={tracectr.h=$(PROJECT_INC_PATH)\tracectr.h}
PASS1_PUBLISH={$(O)\tracectr.lib=$(PROJECT_LIB_PATH)\tracectr.lib}

INCLUDES=$(INCLUDES);       \
        $(BASE_INC_PATH);

#MSC_WARNING_LEVEL=/W4

SOURCES= main.c \
         process.c \
         callbacks.cpp \
         report.c \
         mergetl.c \
         transform.cpp \

C_DEFINES=-DWIN32 -DUNICODE -D_UNICODE

