MAJORCOMP=termsrv
MINORCOMP=regapi

TARGETNAME=regapilib
TARGETPATH=$(TERMSRV_ROOT)\lib\$(_OBJ_DIR)
TARGETTYPE=LIBRARY


INCLUDES = \
    ..\..\..\inc; \

C_DEFINES=$(C_DEFINES) -DREGAPILIB

