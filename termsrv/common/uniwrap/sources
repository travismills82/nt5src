TERMSRV_ROOT = ..\..
TSCOMMON_ROOT = ..

MAJORCOMP=common
MINORCOMP=uniwrap

TARGETNAME=uniwrap
TARGETPATH=$(TERMSRV_ROOT)\lib\$(_OBJ_DIR)
TARGETTYPE=LIBRARY

C_DEFINES=$(C_DEFINES) -DDLL_TRACE $(CLIENT_DLL_CDEFS) -D_MBCS -DUNICODE -D_UNICODE

INCLUDES=$(INCLUDES);  \
         $(TSCOMMON_ROOT)\inc;

#
# List of sources
#

SOURCES = \
    strinout.cpp \
    uniwrap.cpp  \
    prnwrap.cpp  \
    uniansi.cpp  \
    wraputl.cpp  \

