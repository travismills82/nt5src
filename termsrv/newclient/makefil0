all: idfile
!if "$(_TGTOS)" != "CE"
!if "$(_NTX86TREE)" != ""
    binplace -R $(_NTX86TREE)\TSClient -P Placefil.txt idfile
!else if "$(_NTAMD64TREE)" != ""
    binplace -R $(_NTAMD64TREE)\TSClient -P Placefil.txt idfile
!else if "$(_NTIA64TREE)" != ""
    binplace -R $(_NTIA64TREE)\TSClient -P Placefil.txt idfile
!endif
!endif

clean: all
